<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>com.magic.project</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>

	</welcome-file-list>

	<context-param>
		<param-name>manager</param-name>
		<param-value>punith sir</param-value>
	</context-param>
	<servlet>
		<servlet-name>first</servlet-name>
		<servlet-class>magic.firstservlet</servlet-class>

		<init-param>
			<param-name>lead</param-name>
			<param-value>ayush</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>first</servlet-name>
		<url-pattern>/f</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>second</servlet-name>
		<servlet-class>magic.secondservlet</servlet-class>

		<init-param>
			<param-name>lead</param-name>
			<param-value>blessina</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>second</servlet-name>
		<url-pattern>/s</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>data</servlet-name>
	<servlet-class>magic.dataservlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>data</servlet-name>
	<url-pattern>/d</url-pattern>
	</servlet-mapping>
	
	
	
	
</web-app>